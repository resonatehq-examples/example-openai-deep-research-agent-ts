{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"outDir": "dist",
		"rootDir": ".",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"types": ["node"]
	},
	"include": ["index.ts", "agent.ts"]
}
